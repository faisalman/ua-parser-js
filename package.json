{
  "title": "UAParser.js",
  "name": "ua-parser-js",
  "version": "0.7.10",
  "author": "Faisal Salman <fyzlman@gmail.com> (http://faisalman.com)",
  "description": "Lightweight JavaScript-based user-agent string parser",
